{
  "buildCommand": "cd ../.. && pnpm install --frozen-lockfile && pnpm --filter @sim4d/studio... run build",
  "outputDirectory": "dist",
  "framework": null,
  "installCommand": "pnpm install --frozen-lockfile",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        }
      ]
    }
  ]
}
