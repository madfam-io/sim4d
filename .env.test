# Test Environment Configuration
# This file is used by E2E tests to configure services for testing

# Collaboration Server - Disable rate limiting for E2E tests
ENABLE_RATE_LIMIT=false
MAX_CONNECTIONS_PER_IP=100

# Collaboration WebSocket Configuration
COLLAB_PORT=8080
HEARTBEAT_INTERVAL=30000
LOCK_TIMEOUT=60000

# CORS Configuration
CORS_ORIGIN=http://localhost:5173

# Studio Configuration
VITE_API_BASE_URL=http://localhost:8080
VITE_COLLABORATION_WS_URL=http://localhost:8080
VITE_COLLABORATION_API_URL=http://localhost:8080
VITE_ENABLE_COLLABORATION=true
VITE_ENABLE_MONITORING=false

# WASM Configuration
ENABLE_MOCK_GEOMETRY=false
REQUIRE_REAL_OCCT=true
VITE_WASM_WORKERS=true

# Node Environment
NODE_ENV=test
