{
  "name": "@sim4d/engine-core",
  "version": "0.1.0",
  "description": "Sim4D DAG evaluation engine",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./geometry-api-factory": {
      "types": "./dist/geometry-api-factory.d.ts",
      "import": "./dist/geometry-api-factory.mjs",
      "require": "./dist/geometry-api-factory.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@sim4d/types": "workspace:*",
    "isolated-vm": "^6.0.2",
    "uuid": "^9.0.1",
    "xxhash-wasm": "^1.0.2"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.7",
    "tsup": "^8.5.1",
    "vitest": "^4.0.9"
  }
}
