{
  "extends": ["../../.eslintrc.json"],
  "settings": {
    "react": {
      "version": "999.999.999"
    }
  },
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_|^params$|^ctx$|^inputName$|^nodeId$|^paramName$|^userId$|^sessionId$|^value$|^config$|^event$|^listener$|^callback$|^message$|^urlOrSessionId$|^lastKnownVersion$",
        "varsIgnorePattern": "^_|^variables$|^initHasher$|^NodeInstance$|^Constraint$|^GeometryElement$|^MockWebSocketClient$|^createTestSelection$|^waitFor$|^Edge$|^NodeId$|^target$|^client[0-9]$|^config$|^hasOperationMessage$|^Operation$|^DeleteEdgeOperation$"
      }
    ],
    "@typescript-eslint/no-var-requires": "warn",
    "prefer-const": "warn",
    "react/react-in-jsx-scope": "off",
    "react-hooks/rules-of-hooks": "off",
    "react/prop-types": "off"
  }
}
