{
  "name": "@brepflow/types",
  "version": "0.1.0",
  "description": "BrepFlow shared TypeScript types",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@types/dompurify": "^3.2.0",
    "tsup": "^8.5.1"
  },
  "dependencies": {
    "dompurify": "^3.3.0"
  }
}
